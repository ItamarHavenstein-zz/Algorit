package Despertador;

import java.util.Date;

public interface Relogio {

	void setHorario(Date horario);
	void getHorario();
	void setHorarioAlarme(Date horarioalarme);
	void getHorarioAlarme();
	void LigarAlarme();
	void DesligarAlarme();
	void setVolumeRelogio(int volRelogio);
	void getVolumeRelogio();
	
}
